<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
 <mapper namespace="com.zest.client_point">
 
 	<!-- client point  -->
 	<insert id="insertCP" parameterType="Client_pointVO">
 	 INSERT INTO CLIENT_POINT(
					cl_no,
					to_no,
					cp_type,
					cp_in_point,
					cp_out_point,
					cp_total_point
					) 
 	 							VALUES (
 	 									#{cl_no},
 	 									#{to_no}, 
 	 									#{cp_type},
 	 									#{cp_in_point},
 	 									#{cp_out_point},
 	 									#{cp_total_point} 	 									
 	 									)
 	</insert> 	 
 </mapper>